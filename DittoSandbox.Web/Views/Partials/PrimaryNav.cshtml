@model IEnumerable<TreeNode>
@if (Model.Any())
{
    <p style="background-color: #efefef;padding:5px;">
        @foreach (var item in Model)
        {
            <a href="@item.Url">@item.Name</a>

            if (item != Model.Last())
            {
                <text> | </text>
            }
        }
    </p>
}